<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:key="@string/pref_category_private_settings_key"
        android:title="@string/pref_category_private_settings_title">

        <EditTextPreference
            android:defaultValue=""
            android:dialogTitle="@string/pref_private_appid_dialog_title"
            android:inputType="text"
            android:key="@string/pref_private_appid_key"
            android:title="@string/pref_private_appid_title" />

        <EditTextPreference
            android:defaultValue=""
            android:dialogTitle="@string/pref_private_authley_dialog_title"
            android:inputType="text"
            android:key="@string/pref_private_authkey_key"
            android:title="@string/pref_private_authkey_title" />

        <EditTextPreference
            android:dialogTitle="@string/pref_private_user_host_dialog_title"
            android:inputType="text"
            android:key="@string/pref_private_user_host_key"
            android:title="@string/pref_private_user_host_title" />

        <EditTextPreference
            android:defaultValue=""
            android:dialogTitle="@string/pref_private_sls_url_dialog_title"
            android:inputType="text"
            android:key="@string/pref_private_sls_url_key"
            android:title="@string/pref_private_sls_url_title" />

        <EditTextPreference
            android:defaultValue=""
            android:dialogTitle="@string/pref_private_dispatch_url_dialog_title"
            android:inputType="text"
            android:key="@string/pref_private_dispatch_url_key"
            android:title="@string/pref_private_dispatch_url_title" />

    </PreferenceCategory>

    <PreferenceCategory
        android:key="@string/pref_category_more_settings_key"
        android:title="@string/pref_category_more_settings_title">
        <SwitchPreference
            android:defaultValue="@bool/pref_more_config_page_debug_default"
            android:key="@string/pref_more_config_page_debug_key"
            android:title="@string/pref_more_config_page_debug_title" />
        <PreferenceScreen
            android:key="@string/pref_more_config_file_path_key"
            android:title="@string/pref_more_config_file_path_title" />
    </PreferenceCategory>

</PreferenceScreen>
