<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textSize">20dp</item>
    </style>

    <style name="item_style" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textSize">16dp</item>
        <item name="colorPrimary">@color/navigation_menu_item_color</item>
    </style>

    <!--底部tab导航的样式-->
    <style name="tab_style">
        <item name="android:layout_width">@dimen/dimens45</item>
        <item name="android:layout_height">@dimen/dimens45</item>
        <item name="android:background">@drawable/circle_place_selector</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dimens11</item>
    </style>

    <style name="button_style">
        <item name="android:layout_width">@dimen/dimens38</item>
        <item name="android:layout_height">@dimen/dimens38</item>
        <item name="android:background">@mipmap/submitn</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dimens9</item>
    </style>

    <style name="tab_text">
        <item name="android:layout_width">@dimen/dimens45</item>
        <item name="android:layout_height">@dimen/dimens45</item>
        <item name="android:background">@mipmap/cancel_press</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginLeft">@dimen/dimens5</item>
        <item name="android:text">...</item>
        <item name="android:textSize">@dimen/dimens11</item>
    </style>

    <!--下注方位的样式-->
    <style name="place_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/rect_place_selector</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="setting_popwindow_anim_style">
        <item name="android:windowEnterAnimation">@anim/setting_panel_show_animation</item>
        <item name="android:windowExitAnimation">@anim/setting_panel_hide_animation</item>
    </style>
</resources>
